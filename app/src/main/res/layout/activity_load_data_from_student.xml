<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <ScrollView
        android:id="@+id/activity_load_data_from_students_sv"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintVertical_bias="0">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/activity_load_data_from_students_cl"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/activity_load_data_from_student_tv_id"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:hint="ID"
                android:padding="8dp"
                android:layout_margin="8dp"
                app:layout_constraintStart_toStartOf="@+id/activity_load_data_from_students_cl"
                app:layout_constraintEnd_toEndOf="@+id/activity_load_data_from_students_cl"
                app:layout_constraintTop_toTopOf="@+id/activity_load_data_from_students_cl"
                app:layout_constraintBottom_toBottomOf="@+id/activity_load_data_from_students_cl"
                app:layout_constraintVertical_bias="0"/>

            <TextView
                android:id="@+id/activity_load_data_from_student_tv_name"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:hint="Name"
                android:padding="8dp"
                android:layout_margin="8dp"
                app:layout_constraintStart_toStartOf="@+id/activity_load_data_from_students_cl"
                app:layout_constraintEnd_toEndOf="@+id/activity_load_data_from_students_cl"
                app:layout_constraintTop_toBottomOf="@+id/activity_load_data_from_student_tv_id"
                app:layout_constraintBottom_toBottomOf="@id/activity_load_data_from_students_cl"
                app:layout_constraintVertical_bias="0"/>

            <TextView
                android:id="@+id/activity_load_data_from_student_tv_age"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="Age"
                android:padding="8dp"
                android:layout_margin="8dp"
                app:layout_constraintStart_toStartOf="@+id/activity_load_data_from_students_cl"
                app:layout_constraintEnd_toEndOf="@+id/activity_load_data_from_students_cl"
                app:layout_constraintTop_toBottomOf="@+id/activity_load_data_from_student_tv_name"
                app:layout_constraintBottom_toBottomOf="@+id/activity_load_data_from_students_cl"
                app:layout_constraintVertical_bias="0"/>

            <TextView
                android:id="@+id/activity_load_data_from_student_tv_phone"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="Phone"
                android:padding="8dp"
                android:layout_margin="8dp"
                app:layout_constraintStart_toStartOf="@+id/activity_load_data_from_students_cl"
                app:layout_constraintEnd_toEndOf="@+id/activity_load_data_from_students_cl"
                app:layout_constraintTop_toBottomOf="@+id/activity_load_data_from_student_tv_age"
                app:layout_constraintBottom_toBottomOf="@+id/activity_load_data_from_students_cl"
                app:layout_constraintVertical_bias="0"/>

            <TextView
                android:id="@+id/activity_load_data_from_student_tv_email"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="Email"
                android:padding="8dp"
                android:layout_margin="8dp"
                app:layout_constraintStart_toStartOf="@+id/activity_load_data_from_students_cl"
                app:layout_constraintEnd_toEndOf="@+id/activity_load_data_from_students_cl"
                app:layout_constraintTop_toBottomOf="@+id/activity_load_data_from_student_tv_phone"
                app:layout_constraintBottom_toBottomOf="@+id/activity_load_data_from_students_cl"
                app:layout_constraintVertical_bias="0"/>

            <TextView
                android:id="@+id/activity_load_data_from_student_tv_gender"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="Gender"
                android:padding="8dp"
                android:layout_margin="8dp"
                app:layout_constraintStart_toStartOf="@+id/activity_load_data_from_students_cl"
                app:layout_constraintEnd_toEndOf="@+id/activity_load_data_from_students_cl"
                app:layout_constraintTop_toBottomOf="@+id/activity_load_data_from_student_tv_email"
                app:layout_constraintBottom_toBottomOf="@+id/activity_load_data_from_students_cl"
                app:layout_constraintVertical_bias="0"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>